{
    "scripts": {
        "_build_backend_": "go build",
        "_build_backend_dev_": "go build -race",
        "_build_docs_": "npx elm-doc-preview --output ./documentation.json",
        "_build_frontend_": "npx webpack --config config/webpack.prod.js",
        "_build_frontend_dev_": "npx webpack --config config/webpack.dev.js",
        "_dev_backend_": "go run -race main.go --dev",
        "_dev_frontend_": "npx webpack serve --port 8122 --config config/webpack.dev.js",
        "_generate_elm_": "npx ts-node --compilerOptions {\\\"strict\\\":false} ./lib/ts/elm-tailwind-modules/cli.ts --tailwind-config ./config/tailwind.config.js --dir ./generated/elm",
        "_generate_model_": "go run github.com/sqlc-dev/sqlc/cmd/sqlc@latest generate --file ./src/sql/sqlc.yaml",
        "_test_elm_": "npx elm-verify-examples && npx elm-test --fuzz 250",
        "_test_go_": "go test -race",
        "_review_elm_": "npx elm-review",
        "_review_go_": "go vet",
        "_review_model_": "go run github.com/sqlc-dev/sqlc/cmd/sqlc@latest vet --file ./src/sql/sqlc.yaml",
        "build": "npm run _build_frontend_ && npm run _build_backend_",
        "build_dev": "npm run _build_frontend_dev_ && npm run _build_backend_dev_",
        "dev": "npm run build_dev && npx concurrently --kill-others \"npm run _dev_frontend_\" \"npm run _dev_backend_\"",
        "docs": "npx elm-doc-preview",
        "fix": "npx elm-review --fix-all",
        "generate": "npm run _generate_elm_ && npm run _generate_model_",
        "install": "go install github.com/sqlc-dev/sqlc/cmd/sqlc@latest",
        "prepare": "husky",
        "review": "npm run _review_elm_ && npm run _review_go_ && npm run _review_model_",
        "test": "npm run _test_elm_ && npm run review && npm run build && npm run _test_go_"
    },
    "devDependencies": {
        "@tailwindcss/aspect-ratio": "^0.4.2",
        "@tailwindcss/typography": "^0.5.10",
        "@types/color-name": "^1.1.3",
        "@types/deep-equal": "^1.0.4",
        "@types/lodash": "^4.14.202",
        "autoprefixer": "^10.4.16",
        "clean-webpack-plugin": "^4.0.0",
        "concurrently": "^8.2.2",
        "copy-webpack-plugin": "^11.0.0",
        "css-loader": "^6.8.1",
        "css-minimizer-webpack-plugin": "^5.0.1",
        "daisyui": "^4.6.0",
        "elm-format": "^0.8.7",
        "elm-reloader": "^1.0.0",
        "elm-review": "^2.10.3",
        "elm-tailwind-modules": "^0.5.0",
        "elm-test": "^0.19.1-revision12",
        "elm-verify-examples": "^6.0.2",
        "elm-webpack-loader": "^8.0.0",
        "html-webpack-plugin": "^5.6.0",
        "mini-css-extract-plugin": "^2.7.6",
        "postcss": "^8.4.32",
        "postcss-loader": "^7.3.3",
        "sass": "^1.69.5",
        "sass-loader": "^13.3.2",
        "style-loader": "^3.3.3",
        "tailwindcss": "^3.4.0",
        "terser-webpack-plugin": "^5.3.9",
        "ts-node": "^10.9.2",
        "url-loader": "^4.1.1",
        "webpack": "^5.89.0",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^4.15.1",
        "webpack-merge": "^5.10.0"
    },
    "engines": {
        "node": ">=18"
    },
    "prettier": {
        "tabWidth": 4
    },
    "dependencies": {
        "elm": "^0.19.1-6",
        "elm-doc-preview": "^5.0.5",
        "husky": "^9.0.10"
    }
}
