{
	"Prettier-SQL.SQLFlavourOverride": "sqlite",
	"Prettier-SQL.commaPosition": "tabular",
	"Prettier-SQL.denseOperators": false,
	"Prettier-SQL.expressionWidth": 80,
	"Prettier-SQL.ignoreTabSettings": true,
	"Prettier-SQL.indentStyle": "standard",
	"Prettier-SQL.insertSpacesOverride": true,
	"Prettier-SQL.keywordCase": "upper",
	"Prettier-SQL.linesBetweenQueries": 2,
	"Prettier-SQL.logicalOperatorNewline": "after",
	"Prettier-SQL.newlineBeforeSemicolon": false,
	"Prettier-SQL.tabSizeOverride": 4,
	"Prettier-SQL.tabulateAlias": true,
	"cSpell.language": "en,de",
	"cSpell.words": [
		"github",
		"gqlgen",
		"httprouter",
		"julienschmidt",
		"mattn",
		"sqlite"
	],
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.formatOnSave": true,
	"files.eol": "\n",
	"git.autofetch": true,
	"go.coverOnSave": true,
	"go.diagnostic.vulncheck": "Imports",
	"go.inlayHints.assignVariableTypes": true,
	"go.inlayHints.compositeLiteralFields": true,
	"go.inlayHints.compositeLiteralTypes": true,
	"go.inlayHints.constantValues": true,
	"go.inlayHints.rangeVariableTypes": true,
	"go.survey.prompt": false,
	"go.testExplorer.packageDisplayMode": "nested",
	"go.testExplorer.showDynamicSubtestsInEditor": true,
	"line-length-checker.blacklist": ["*.md", "node_modules/**"],
	"line-length-checker.level": "Info",
	"line-length-checker.lineLength": 80,
	"prettier.arrowParens": "always",
	"prettier.bracketSameLine": false,
	"prettier.bracketSpacing": true,
	"prettier.configPath": ".prettierrc",
	"prettier.embeddedLanguageFormatting": "auto",
	"prettier.enable": true,
	"prettier.enableDebugLogs": false,
	"prettier.endOfLine": "lf",
	"prettier.htmlWhitespaceSensitivity": "css",
	"prettier.ignorePath": ".prettierignore",
	"prettier.insertPragma": false,
	"prettier.jsxSingleQuote": false,
	"prettier.prettierPath": "",
	"prettier.printWidth": 80,
	"prettier.proseWrap": "always",
	"prettier.quoteProps": "consistent",
	"prettier.requireConfig": true,
	"prettier.requirePragma": false,
	"prettier.resolveGlobalModules": false,
	"prettier.semi": true,
	"prettier.singleAttributePerLine": false,
	"prettier.singleQuote": false,
	"prettier.tabWidth": 4,
	"prettier.trailingComma": "all",
	"prettier.useEditorConfig": true,
	"prettier.useTabs": true,
	"prettier.vueIndentScriptAndStyle": false,
	"prettier.withNodeModules": false,
	"sortLines.filterBlankLines": true,
	"[elm]": {
		"editor.defaultFormatter": "elmTooling.elm-ls-vscode"
	},
	"[go]": {
		"editor.defaultFormatter": "golang.go"
	},
	"[sql]": {
		"editor.defaultFormatter": "inferrinizzard.prettier-sql-vscode"
	}
}
